<div class="song-main-container">

  <div id="side-nav" class="col-md-2" role="navigation">
    <div class="button-holder">
      <%= link_to 'New Song', new_song_path %>
    </div>
  </div> <!--nav bar on top of page-->

  <div class="inner-container col-md-10">
    <!-- <h2 class="list-song-title-1">My Library</h2> -->
    <h4 class="list-song-title-2">List of Songs</h4>

    <div class="row row-header">
      <h5 class="col-md-8">Title</h5>
      <h5 class="col-md-4">Owner</h5>
    </div> <!--headers under List of Songs-->

    <% @songs.each do |song| %>
      <div class="row song-list-container">
        <div class="col-md-8 song-title"><a href="songs/<%= song.id %>" class="show-song"><%= song.title %></a></div>
        <div class="col-md-4">
          <p class="user-name"><%= song.user.first_name %></p>
        </div>
      </div>
    <% end %>
  </div>

</div>

